REACT_APP_BYPASS_AUTH=true

# Firebase configuration
# Ces valeurs doivent être remplacées par les vraies clés de votre projet Firebase
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=1:your-app-id
REACT_APP_FIREBASE_MEASUREMENT_ID=G-measurement-id

# Région pour les Cloud Functions (par défaut: us-central1)
REACT_APP_FIREBASE_REGION=us-central1

# Environnement
NODE_ENV=development

